# the image to use
GOTIFY_IMAGE="gotify/server"

# the name of the container
GOTIFY_CONTAINER="gotify"

# the host name configured by GOTIFY
GOTIFY_HOST='gotify.{{get-env "NET_DOMAIN"}}'

# the last two blocks of the ipv4 address
GOTIFY_IP_BLOCK="0.14"

# default password
GOTIFY_DEFAULTUSER_PASS="{{{new-password 'gotify/defaultuser/pass' 16}}}"