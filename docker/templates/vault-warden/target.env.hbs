# the image to use
VW_IMAGE="vaultwarden/server:latest"

# the name of the container
VW_CONTAINER="vault-warden"

# the host name configured by VW
VW_HOST='vw.{{get-env "NET_DOMAIN"}}'

# the last two blocks of the ipv4 address
VW_IP_BLOCK="0.9"

# default password
VW_ADMIN_TOKEN="{{{new-password 'vault-warden/admin/token' 16}}}"

# options
VW_WEBSOCKET_ENABLED=true
VW_WEB_VAULT_ENABLED=true
VW_SIGNUPS_ALLOWED=false