# the image to use
WT_IMAGE="containrrr/watchtower"

# the name of the container
WT_CONTAINER="watchtower"

# the last two blocks of the ipv4 address
WT_IP_BLOCK="0.3"

WT_CLEANUP=true 
WT_SCHEDULE="0 0 4 * * MON"

WT_MONITOR_ONLY=false
WT_POLL_INTERVAL=30

WT_NOTIFICATIONS=gotify
WT_GOTIFY_URL="gotify.{{get-env "NET_DOMAIN"}}"
WT_GOTIFY_TOKEN="{{{new-password 'gotify/defaultuser/pass' 16}}}"